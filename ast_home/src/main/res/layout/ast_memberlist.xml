<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0C000000">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <TextView
                android:id="@+id/ast_memberlist_t001"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_marginStart="@dimen/padding15"
                android:layout_marginTop="30dp"
                android:background="@color/White"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/padding10"
                android:text="@string/ast_more_login_account"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_e001"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_alignTop="@+id/ast_memberlist_t001"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="@dimen/padding15"
                android:layout_toEndOf="@+id/ast_memberlist_t001"
                android:autofillHints=""
                android:background="@color/White"
                android:backgroundTint="@android:color/background_light"
                android:ems="10"
                android:gravity="center|center_vertical"
                android:inputType="textWebEditText"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_t003"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_below="@+id/ast_memberlist_t001"
                android:layout_alignParentStart="true"
                android:layout_marginStart="@dimen/padding15"
                android:layout_marginTop="10dp"
                android:background="@color/White"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/padding15"

                android:text="@string/ast_memberlist_name"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_e003"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_alignTop="@+id/ast_memberlist_t003"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="@dimen/padding15"
                android:layout_toEndOf="@+id/ast_memberlist_t003"
                android:background="@color/White"
                android:backgroundTint="@android:color/background_light"
                android:ems="10"
                android:gravity="center|center_vertical"
                android:importantForAutofill="no"
                android:inputType="date"
                android:textSize="18sp"
                android:visibility="visible" />

            <TextView
                android:id="@+id/ast_memberlist_t004"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_below="@+id/ast_memberlist_t003"
                android:layout_alignParentStart="true"
                android:layout_marginStart="@dimen/padding15"
                android:layout_marginTop="10dp"
                android:background="@color/White"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/padding15"

                android:text="@string/ast_memberlist_sex"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_e004"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_alignTop="@+id/ast_memberlist_t004"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="@dimen/padding15"
                android:layout_toEndOf="@+id/ast_memberlist_t004"
                android:background="@color/White"
                android:backgroundTint="@android:color/background_light"
                android:ems="10"
                android:gravity="center_vertical"
                android:importantForAutofill="no"
                android:inputType="numberDecimal"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_t005"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_below="@+id/ast_memberlist_t004"
                android:layout_alignParentStart="true"
                android:layout_marginStart="@dimen/padding15"
                android:layout_marginTop="10dp"
                android:background="@color/White"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/padding15"

                android:text="@string/ast_memberlist_bitrh"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_e005"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_alignTop="@+id/ast_memberlist_t005"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="@dimen/padding15"
                android:layout_toEndOf="@+id/ast_memberlist_t005"
                android:background="@color/White"
                android:backgroundTint="@android:color/background_light"
                android:ems="10"
                android:gravity="center|center_vertical"
                android:inputType="textPostalAddress"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_t007"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_below="@+id/ast_memberlist_t005"
                android:layout_alignParentStart="true"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:background="@color/White"
                android:gravity="center_vertical"
                android:paddingStart="@dimen/padding10"

                android:text="@string/ast_memberlist_phone"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/ast_memberlist_e008"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_alignTop="@+id/ast_memberlist_t007"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="@dimen/padding15"
                android:layout_toEndOf="@+id/ast_memberlist_t007"
                android:background="@color/White"
                android:backgroundTint="@android:color/background_light"
                android:ems="10"
                android:gravity="center|center_vertical"
                android:inputType="textEmailAddress"
                android:textSize="18sp" />

            <RadioGroup
                android:id="@+id/radiosex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/ast_memberlist_e004"
                android:layout_marginStart="30dp"
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/ast_memberlist_t004"
                android:orientation="horizontal">

                <RadioButton
                    android:id="@+id/radioman"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="0dp"
                    android:text="@string/radioman" />

                <RadioButton
                    android:id="@+id/radiowoman"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="0dp"
                    android:text="@string/radiowoman" />
            </RadioGroup>

            <Button
                android:id="@+id/ast_memberlist_btnOK"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_below="@+id/ast_memberlist_t007"
                android:layout_alignStart="@+id/ast_memberlist_t007"
                android:layout_marginStart="0dp"
                android:layout_marginTop="85dp"
                android:text="@string/ast_point_b002" />

            <Button
                android:id="@+id/ast_memberlist_btnCancel"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_below="@+id/ast_memberlist_e008"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="85dp"
                android:layout_marginEnd="15dp"
                android:text="@string/ast_more_login_canclebtn" />

        </RelativeLayout>
    </ScrollView>

</RelativeLayout>