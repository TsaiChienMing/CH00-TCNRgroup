<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Ast_parking">

    <SearchView
        android:id="@+id/ast_parking_s001"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </SearchView>

    <TableRow
        android:id="@+id/ast_parking_tab01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/ast_parking_t002"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@color/cABD3D4"
            android:text="@string/ast_parking_t002"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/ast_parking_t003"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@color/cABD3D4"
            android:text="@string/ast_parking_t003"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/ast_parking_t004"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:background="@color/cABD3D4"
            android:text="@string/ast_parking_t004"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/ast_parking_t005"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/cABD3D4"
            android:text="@string/ast_parking_t005"
            android:textStyle="bold" />

    </TableRow>

    <ListView
        android:id="@+id/listView1"
        android:layout_width="match_parent"
        android:layout_height="450dp"
        android:background="@color/White" />

</LinearLayout>